    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "next": "^16.0.10",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "object-hash": "^3.0.0",
    "prisma": "^6.19.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "swagger-ui-react": "^5.30.2",
