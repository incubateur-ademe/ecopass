{
  "name": "ecopass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "predev": "react-dsfr update-icons",
    "prebuild": "pnpm prisma:generate && react-dsfr update-icons && pnpm ecobalyse:data",
    "prisma:generate": "prisma generate",
    "queue:watch": "tsx watch queue.ts",
    "queue": "tsx queue.ts",
    "test": "jest && playwright test",
    "ecobalyse:data": "tsx src/scripts/decryptEcobalyseData.ts",
    "reset:test": "DATABASE_URL=postgresql://ecopass_test:ecopass_test@localhost:5433/ecopass_test pnpm prisma migrate reset --force --skip-seed",
    "preinstall": "npx -y only-allow@1.2.2 pnpm"
  },
  "prisma": {
    "seed": "tsx prisma/seed/index.ts"
  },
  "engines": {
    "node": "^22.0.0",
    "pnpm": "10.x"
  },
